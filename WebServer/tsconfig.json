{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "strict": true,
    "strictNullChecks": true,
    "lib": [
      "dom",
      "es5",
      "es2015.promise",
      "es2016",
      "es2017"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "removeComments": false,
    "sourceMap": true,
    "target": "es5"
  }
}
